{
  "name": "event-management-system",
  "version": "1.0.0",
  "description": "A full-stack event management system",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "echo 'Building Vercel serverless functions'",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run dev:frontend\"",
    "start:backend": "cd backend && npm start"
  },
  "keywords": [
    "event-management",
    "mern",
    "mongodb",
    "express",
    "react",
    "nodejs"
  ],
  "author": "owaisworkers",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
