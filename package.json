{
  "name": "event-management-system",
  "version": "1.0.0",
  "description": "A full-stack event management system",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm install --prefix frontend && npm run build --prefix frontend",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run dev:frontend\"",
    "start:backend": "cd backend && npm start"
  },
  "keywords": [
    "event-management",
    "mern",
    "mongodb",
    "express",
    "react",
    "nodejs"
  ],
  "author": "owaisworkers",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "socket.io": "^4.8.1",
    "validator": "^13.15.1"
  }
}